{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "./main.py"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./main.py"
        }
    ],
    
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ]
}